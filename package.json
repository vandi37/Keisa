{
  "name": "simple-documentation",
  "displayName": "Simple documentation",
  "description": "A simple documentation format",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "keisa",
        "aliases": [
          "Keisa",
          "keisa"
        ],
        "extensions": [
          ".keisa"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "keisa",
        "scopeName": "text.keisa",
        "path": "./syntaxes/keisa.tmLanguage.json"
      }
    ]
  }
}